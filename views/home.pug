extends ./layout.pug

block content
  h1 Home
  .row
    each prnt in printProducts
      .col
        .card(style='width: 18rem;')
          img.card-img-top(src=prnt.image alt='...')
          .card-body
            h5.card-title=prnt.name
            p.card-text=prnt.desc
            p=prnt.price
            a.btn.btn-primary(href='#') Buy Now!